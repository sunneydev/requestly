{
  "name": "@sunney/requests",
  "version": "0.1.12",
  "description": "A minimal, but powerful HTTP Client for Node.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunneydev/requests.git"
  },
  "homepage": "https://github.com/sunneydev/requests#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm --dts-resolve",
    "build": "pnpm run build-fast --",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "tsup": "6.5.0",
    "typescript": "4.9.3"
  }
}
