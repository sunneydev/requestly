{
  "name": "@sunney/requests",
  "version": "0.2.0",
  "description": "A minimal, but powerful HTTP Client for Node.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "fetch",
    "axios",
    "requests",
    "http-client",
    "api-client",
    "http-request",
    "node-fetch",
    "got",
    "isomorphic-fetch",
    "networking"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunneydev/requests.git"
  },
  "type": "module",
  "module": "dist/index.mjs",
  "homepage": "https://github.com/sunneydev/requests#readme",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsup src/dev.ts --sourcemap --watch --format esm --onSuccess \"node dist/dev.js\"",
    "build": "tsup src/index.ts --format cjs,esm --dts-resolve --minify",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  }
}
